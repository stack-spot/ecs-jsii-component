{
  "name": "@stackspot/cdk-env-container",
  "version": "0.1.7",
  "description": "Component to create a container environment.",
  "keywords": [
    "aws-cdk",
    "container"
  ],
  "bugs": "https://github.com/stack-spot/container-env-jsii-component/issues",
  "license": "MIT",
  "author": {
    "name": "StackSpot",
    "email": "contact@stackspot.com"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/stack-spot/container-env-jsii-component.git"
  },
  "scripts": {
    "build": "jsii --verbose --fail-on-warnings",
    "build:watch": "npm run build -- --watch",
    "lint": "eslint --ext=js,ts --max-warnings=0 .",
    "lint:fix": "npm run lint -- --fix",
    "package:dotnet": "jsii-pacmak --target=dotnet --verbose --validate-assemblies",
    "package:js": "jsii-pacmak --target=js --verbose --validate-assemblies",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.13",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^27.4.5",
    "jsii": "^1.49.0",
    "jsii-pacmak": "^1.49.0",
    "ts-jest": "^27.1.1",
    "typescript": "~4.5.4"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testEnvironment": "node",
    "roots": ["<rootDir>/test"],
    "testMatch": ["**/*.test.ts"],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "verbose": true
  },
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "full",
    "targets": {
      "dotnet": {
        "namespace": "StackSpot.Env.Container",
        "packageId": "StackSpot.Env.Container",
        "iconUrl": "https://www.nuget.org/profiles/stackspot/avatar"
      }
    }
  }
}
